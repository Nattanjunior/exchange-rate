generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ExchangeRate {
  id        Int      @id @default(autoincrement())
  currency  String
  rate      Decimal  @db.Decimal(20, 10)
  quotedAt  DateTime
  createdAt DateTime @default(now())
}
